{
	"cli": {
		"version": ">= 16.2.2",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"channel": "development"
		},
		"preview": {
			"autoIncrement": true,
			"channel": "preview"
		},
		"preview-local": {
			"channel": "preview",
			"distribution": "store",
			"autoIncrement": true,
			"ios": {
				"buildConfiguration": "Release"
			},
			"android": {
				"buildType": "app-bundle"
			}
		},
		"production": {
			"autoIncrement": true,
			"channel": "production"
		},
		"e2e-test": {
			"withoutCredentials": true,
			"ios": {
				"simulator": true
			},
			"android": {
				"buildType": "apk"
			}
		}
	},
	"submit": {
		"production": {},
		"preview": {}
	}
}
